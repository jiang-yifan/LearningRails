<h1><%= @track.name %></h1>
<%= button_to "Edit Track", edit_track_url(@track), method: :get %>
<%= button_to "Delete Track", track_url(@track), method: :delete %>
<%= button_to "Back to Album", album_url(@track.album_id), method: :get %>
<%= button_to "New Track", new_track_url, method: :get %>
<br>

<pre><%= ugly_lyrics @track.lyrics %></pre>


<%= render "notes/note", track: @track %>
